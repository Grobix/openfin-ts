{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "rules": {
    "no-switch-case-fall-through": false,
    "no-console": [
      true
    ],
    "max-classes-per-file": false,
    "ter-arrow-parens": false,
    "prefer-for-of": true,
    "prefer-const": true,
    "max-line-length": false,
    "prefer-template": false,
    "no-namespace": false,
    "align": false,
    "forin": false,
    "no-unused-variable": [
      true
    ]
  }
}
